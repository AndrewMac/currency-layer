{
    "name": "spmcommercial/currency-layer",
    "description": "API integration with https://currencylayer.com/",
    "type": "craft-plugin",
    "version": "1.0.4",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "currencylayer"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "spmcommercial",
            "homepage": "https://spm-commercial.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "spmcommercial\\currencylayer\\": "src/"
        }
    },
    "extra": {
        "name": "Currency Layer",
        "handle": "currency-layer",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/spm-commercial/currency-layer/master/CHANGELOG.md",
        "components": {
            "api": "spmcommercial\\currencylayer\\services\\Api",
            "currency": "spmcommercial\\currencylayer\\services\\Currency"
        },
        "class": "spmcommercial\\currencylayer\\CurrencyLayer"
    }
}
